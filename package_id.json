{
  "name": "language-id-example",
  "displayName": "Language ID Example",
  "description": "An example extension to get the language ID",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.60.0"
  },
  "activationEvents": [
    "onCommand:extension.getLanguageId"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.getLanguageId",
        "title": "Get-Language-ID"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./"
  },
  "devDependencies": {
    "typescript": "^4.0.0",
    "vscode": "^1.1.37",
    "tslint": "^5.20.1",
    "eslint": "^7.32.0",
    "eslint-plugin-vscode": "^1.0.0"
  }
}